{
  "name": "svelte-ssr-example",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "scss-build": "node build-scss.js",
    "scss-watch": "chokidar \"scss/**/*.scss\" -c \"npm run scss-build\"",
    "rollup-build": "rollup -c",
    "rollup-watch": "rollup -c -w",
    "server-watch": "DEV=true nodemon server/index.js",
    "start": "npm run rollup-build && npm run scss-build && node server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://pierbover@github.com/PierBover/svelte-ssr-example.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PierBover/svelte-ssr-example/issues"
  },
  "homepage": "https://github.com/PierBover/svelte-ssr-example#readme",
  "dependencies": {
    "@rollup/plugin-node-resolve": "^7.1.3",
    "autoprefixer": "^9.7.6",
    "chokidar-cli": "^2.1.0",
    "fastify": "^2.13.1",
    "fastify-compress": "^2.0.1",
    "fastify-static": "^2.6.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.1",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "svelte": "^3.20.1",
    "svelte-preprocess": "^3.7.1"
  },
  "nodemonConfig": {
    "ignore": [
      "server/components/*",
      "server/ssr/*",
      "server/public/*"
    ],
    "delay": "1000"
  }
}
